{
  "name": "bootstrap",
  "description": "The most popular front-end framework for developing responsive, mobile first projects on the web.",
  "version": "3.3.7",
  "keywords": [
    "css",
    "less",
    "jQuery",
    "mobile-first",
    "responsive",
    "front-end",
    "framework",
    "web"
  ],
  "homepage": "https://ku-kom.github.io/bootstrap/",
  "author": "University of Copenhagen, based on Twitter Bootstrap, Inc.",
  "scripts": {
    "change-version": "node grunt/change-version.js",
    "update-shrinkwrap": "npm shrinkwrap --dev && shx mv ./npm-shrinkwrap.json ./grunt/npm-shrinkwrap.json",
    "test": "grunt test"
  },
  "style": "dist/css/bootstrap.css",
  "less": "less/bootstrap.less",
  "main": "./dist/js/npm",
  "repository": {
    "type": "git",
    "url": "https://github.com/ku-kom/bootstrap.git"
  },
  "bugs": {
    "url": "https://github.com/ku-kom/bootstrap/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "btoa": "~1.2.1",
    "glob": "~8.0.3",
    "grunt": "^1.5.3",
    "grunt-autoprefixer": "~3.0.4",
    "grunt-contrib-clean": "~2.0.1",
    "grunt-contrib-compress": "^2.0.0",
    "grunt-contrib-concat": "~2.1.0",
    "grunt-contrib-connect": "~3.0.0",
    "grunt-contrib-copy": "~1.0.0",
    "grunt-contrib-csslint": "~2.0.0",
    "grunt-contrib-cssmin": "~4.0.0",
    "grunt-contrib-htmlmin": "~3.1.0",
    "grunt-contrib-jshint": "~3.2.0",
    "grunt-contrib-less": "~3.0.0",
    "grunt-contrib-pug": "~3.0.0",
    "grunt-contrib-uglify": "~5.2.2",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-csscomb": "~4.2.0",
    "grunt-eslint": "^24.0.0",
    "grunt-exec": "~3.0.0",
    "grunt-html": "^15.2.2",
    "grunt-jekyll": "~1.0.1",
    "grunt-stylelint": "^0.18.0",
    "load-grunt-tasks": "~5.1.0",
    "markdown-it": "^13.0.1",
    "shelljs": "^0.8.5",
    "shx": "^0.3.4",
    "stylelint": "^14.10.0",
    "text-table": "^0.2.0",
    "time-grunt": "^2.0.0"
  },
  "engines": {
    "node": ">=0.10.1"
  },
  "files": [
    "dist",
    "fonts",
    "grunt",
    "js/*.js",
    "less/**/*.less",
    "Gruntfile.js",
    "LICENSE"
  ],
  "jspm": {
    "main": "js/bootstrap",
    "shim": {
      "js/bootstrap": {
        "deps": "jquery",
        "exports": "$"
      }
    },
    "files": [
      "css",
      "fonts",
      "js"
    ]
  },
  "dependencies": {
    "grunt-cli": "^1.4.3",
    "gulp": "^4.0.2",
    "npm": "^8.17.0"
  }
}
